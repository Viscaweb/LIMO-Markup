{% set bar_progress = '60' %}

<div class="ls-match-statistics">
    <div class="ls-possesion-graph ls-list">
        <div class="ls-col ls-col--value"></div>
        <div class="ls-col ls-col--graph"></div>
        <div class="ls-col ls-col--value"></div>
        <div class="row row--title">
            <div class="cell cell__title">
                <span>Possesion</span>
            </div>
        </div>
        <div class="row row__data">
            <div class="cell cell__value cell__value--home">
                <span>44%</span>
            </div>
            <div class="cell cell__graph cell__graph--away">
                <div class="bar-container">
                    <div class="bar">
                        <div class="progress" style="width: {{ bar_progress }}%;"></div>
                    </div>
                    <span class="separator"></span>
                </div> 
            </div>
            <div class="cell cell__value cell__value--away">
                <span>56%</span>
            </div>
        </div>
    </div>
    <ul class="ls-match-statistics-list ls-list">
        <li class="row">
            <div class="cell">
                <div class="row">
                    <div class="cell cell__title">
                        <span>Shots on target</span>
                    </div>
                </div>
                <div class="row row__data">
                    <div class="cell cell__value cell__value--home">
                        <span>4</span>
                    </div>
                    <div class="cell cell__graph cell__graph--home">
                        <div class="bar" style="width: 30%;"></div>
                    </div>
                    <div class="cell cell__graph cell__graph--away cell__graph--high">
                        <div class="bar" style="width: 100%;"></div>
                    </div>
                    <div class="cell cell__value cell__value--away">
                        <span>15</span>
                    </div>
                </div>
            </div>
        </li>
        <li class="row">
            <div class="cell">
                <div class="row">
                    <div class="cell cell__title">
                        <span>Shots off target</span>
                    </div>
                </div>
                <div class="row row__data">
                    <div class="cell cell__value cell__value--home">
                        <span>4</span>
                    </div>
                    <div class="cell cell__graph cell__graph--home">
                        <div class="bar" style="width: 35%;"></div>
                    </div>
                    <div class="cell cell__graph cell__graph--away cell__graph--high">
                        <div class="bar" style="width: 100%;"></div>
                    </div>
                    <div class="cell cell__value cell__value--away">
                        <span>15</span>
                    </div>
                </div>
            </div>
        </li>
        <li class="row">
            <div class="cell">
                <div class="row">
                    <div class="cell cell__title">
                        <span>Free kicks</span>
                    </div>
                </div>
                <div class="row row__data">
                    <div class="cell cell__value cell__value--home">
                        <span>10</span>
                    </div>
                    <div class="cell cell__graph cell__graph--home cell__graph--high">
                        <div class="bar" style="width: 100%;"></div>
                    </div>
                    <div class="cell cell__graph cell__graph--away">
                        <div class="bar" style="width: 80%;"></div>
                    </div>
                    <div class="cell cell__value cell__value--away">
                        <span>9</span>
                    </div>
                </div>
            </div>
        </li>
        <li class="row">
            <div class="cell">
                <div class="row">
                    <div class="cell cell__title">
                        <span>Corner</span>
                    </div>
                </div>
                <div class="row row__data">
                    <div class="cell cell__value cell__value--home">
                        <span>5</span>
                    </div>
                    <div class="cell cell__graph cell__graph--home">
                        <div class="bar" style="width: 70%;"></div>
                    </div>
                    <div class="cell cell__graph cell__graph--away cell__graph--high">
                        <div class="bar" style="width: 100%;"></div>
                    </div>
                    <div class="cell cell__value cell__value--away">
                        <span>8</span>
                    </div>
                </div>
            </div>
        </li>
        <li class="row">
            <div class="cell">
                <div class="row">
                    <div class="cell cell__title">
                        <span>Offsides</span>
                    </div>
                </div>
                <div class="row row__data">
                    <div class="cell cell__value cell__value--home">
                        <span>0</span>
                    </div>
                    <div class="cell cell__graph cell__graph--home">
                        <div class="bar" style="width: 0%;"></div>
                    </div>
                    <div class="cell cell__graph cell__graph--away cell__graph--high">
                        <div class="bar" style="width: 100%;"></div>
                    </div>
                    <div class="cell cell__value cell__value--away">
                        <span>3</span>
                    </div>
                </div>
            </div>
        </li>
        <li class="row">
            <div class="cell">
                <div class="row">
                    <div class="cell cell__title">
                        <span>Throw-ins</span>
                    </div>
                </div>
                <div class="row row__data">
                    <div class="cell cell__value cell__value--home">
                        <span>10</span>
                    </div>
                    <div class="cell cell__graph cell__graph--home">
                        <div class="bar" style="width: 50%;"></div>
                    </div>
                    <div class="cell cell__graph cell__graph--away cell__graph--high">
                        <div class="bar" style="width: 100%;"></div>
                    </div>
                    <div class="cell cell__value cell__value--away">
                        <span>25</span>
                    </div>
                </div>
            </div>
        </li>
        <li class="row">
            <div class="cell">
                <div class="row">
                    <div class="cell cell__title">
                        <span>Goalkeeper saves</span>
                    </div>
                </div>
                <div class="row row__data">
                    <div class="cell cell__value cell__value--home">
                        <span>12</span>
                    </div>
                    <div class="cell cell__graph cell__graph--home cell__graph--high">
                        <div class="bar" style="width: 100%;"></div>
                    </div>
                    <div class="cell cell__graph cell__graph--away">
                        <div class="bar" style="width: 10%;"></div>
                    </div>
                    <div class="cell cell__value cell__value--away">
                        <span>1</span>
                    </div>
                </div>
            </div>
        </li>
        <li class="row">
            <div class="cell">
                <div class="row">
                    <div class="cell cell__title">
                        <span>Goal kicks</span>
                    </div>
                </div>
                <div class="row row__data">
                    <div class="cell cell__value cell__value--home">
                        <span>11</span>
                    </div>
                    <div class="cell cell__graph cell__graph--home cell__graph--high">
                        <div class="bar" style="width: 100%;"></div>
                    </div>
                    <div class="cell cell__graph cell__graph--away">
                        <div class="bar" style="width: 80%;"></div>
                    </div>
                    <div class="cell cell__value cell__value--away">
                        <span>9</span>
                    </div>
                </div>
            </div>
        </li>
    </ul>
</div>